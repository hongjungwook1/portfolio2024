<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="deleteBoard">
	<delete id="deleteBoard">
		DELETE 
		FROM 
			NOTICE_HJW
		WHERE
			BOARD_ID = #{board_id} 
	</delete>


</mapper>